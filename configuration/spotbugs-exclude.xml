<FindBugsFilter>
    <!-- Incorrect identification of dead local store -->
    <Match>
        <Class name="com.github.windpapi4j.WinDPAPI" />
        <Method name="raiseHResultExceptionForLastError" />
        <Bug pattern="DLS_DEAD_LOCAL_STORE" />
    </Match>
    <Match>
        <Class name="~.*\DATA_BLOB$" />
        <Bug pattern="MC_OVERRIDABLE_METHOD_CALL_IN_CONSTRUCTOR" />
    </Match>
</FindBugsFilter>